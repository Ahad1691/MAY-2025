#include <iostream>
using namespace std ;

int main (){

    int n;
    cout<< "ENTER THE NUMBER YOU WANT TO CHECK : ";
    cin>>n;

    int i = 2 ;

    while (i<n){
        if(n%i==0){
            cout<<"THIS IS NOT A PRIME NUMBER FOR " << i << endl;
        }
         else {
            cout<<"THIS IS A PRIME NUMBER FOR "<< i <<endl;
            
        }
        i = i + 1 ;
        

    }

   
    
}
