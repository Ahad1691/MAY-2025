#include <iostream>
using namespace std;

int main () {
    
    int n;
    cout<<"ENTER THE DESIRED NUMBER OF SUM : ";
    cin >> n;

    int i = 1;
    int sum = 0;

    while (i<=n){
        
        sum = sum + i;
        i = i+1 ;
    }
    
    cout << "the value of sum is " << sum << endl;


   

}
