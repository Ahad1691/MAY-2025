DATE - 27/05/2025

#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Enter n: ";
    cin >> n;

    for (int i = 1; i <= n; ++i) {
        for (int j = 1; j <= i; ++j) {
            cout << j;
            if (j < i) cout << "*";
        }
        cout << endl;
    }

    return 0;
}
