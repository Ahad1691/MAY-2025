// third c++ program to compare two numbers and tell which one is greater.

#include <iostream>
using namespace std;

int main() {
    int a;
    cout << "PLEASE ENTER THE FIRST NUMBER A = ";
    cin >> a;

    int b;
    cout << "PLEASE ENTER THE SECOND NUMBER B = ";
    cin >> b;

    if (a > b) {
        cout << "A is greater" << endl;
    } else if (b > a) {
        cout << "B is greater" << endl;
    } else {
        cout << "Both numbers are equal" << endl;
    }

    return 0;
}
